(function (b) {
    b.writePhoneNumber = function (k, l) {
        function i(c) {
            c.tip = b(c.tip);
            c.phone_list = b(c.phone_list);
            c.phone1 = b(c.phone1);
            c.phone2 = b(c.phone2)
        }
        function n(d) {
            var f = d.split(/\s?\/\s?/),
                c = null,
                e = 0;
            for (; e < f.length; e++) {
                c = b.trim(f[e]).split(/\s/, 2);
                if (c.length === 2) {
                    f[e] = '<span class="areacode">' + c[0] + '</span> <span class="number">' + c[1] + "</span>"
                }
            }
            return {
                phone_list: f.join(" / "),
                phone1: f[0],
                phone2: f[1]
            }
        }
        function m(c, d) {
            if (c) {
                c.html(d)
            }
        }
        function a(c) {
            m(l.phone_list, c.phone_list);
            m(l.phone1, c.phone1);
            m(l.phone2, c.phone2)
        }
        function j() {
            var c = null;
            i(l);
            c = n(k);
            a(c);
            if (l.tip) {
                l.tip.slideDown()
            }
        }
        j()
    }
}(jQuery));
(function (d, c) {
    c.likeReview = function () {
        var a = d(this);
        a.html("Aguarde...");
        d.post("/local/review/like/" + a.dataset("lbsid") + "/" + a.dataset("review_id"), d.proxy(c.handlePost, this), "json")
    };
    c.handlePost = function (a) {
        var b = d(this);
        if (a.success === true) {
            if (!a.hasOwnProperty("numLikes")) {
                a.numLikes = parseInt(b.dataset("num_likes"), 10) + 1
            }
            b.html('Obrigado pelo elogio! <span title="Obrigado pelo elogio!">(' + a.numLikes + ")</span>").addClass("sprite_ico_up_ajudou_successo").removeClass("sprite_ico_up_ajudou")
        } else {
            b.likeReviewButton()
        }
    };
    d.fn.likeReviewButton = function () {
        this.each(function (k, i) {
            var a = d(i),
                l = a.dataset("review_id"),
                j = a.dataset("lbsid");
            if (l.length && j.length) {
                var b = a.dataset("num_likes") || 0;
                a.addClass("sprite_icones sprite_ico_up_ajudou").removeClass("sprite_ico_up_ajudou_successo").html("Essa avaliação me ajudou <span>(" + b + ")</span>");
                a.click(function (e) {
                    e.preventDefault();
                    d(this).unbind("click");
                    d.proxy(c.likeReview, this)()
                })
            }
        });
        return this
    }
