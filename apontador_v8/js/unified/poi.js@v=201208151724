window.onload = function () {
    jq("#apontaofertas").html('<iframe src="http://widget.apontaofertas.com.br/widget/' + jq("#cityInputNormalized").val() + "?ap_categoryid=" + jq("#categoryId").val() + '&ncoupons=3" scrolling="no" allowtransparency="true" width="100%" height="100%" marginheight="0" marginwidth="0" frameborder="0"></iframe>');
