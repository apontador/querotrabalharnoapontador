    b.fn.dataset = function (d) {
        if (this.length === 0) {
            return null
        }
        if (this.length == 1) {
            if (this[0].dataset !== undefined) {
                return this[0].dataset[d]
            }
            return this.attr("data-" + d)
        }
        var a = {};
        this.each(function (c, f) {
            a[b(f).attr("id")] = b(f).dataset(d)
        });
        return a
    }
