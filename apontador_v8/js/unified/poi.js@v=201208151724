jq(".silverbutton>button").button();
var validateComment = function (g) {
    var f = false;
    if (jq("#" + g + " .stars:checked").length !== 1) {
        jq(".erroResenha_nota").show();
        f = true
    } else {
        jq(".erroResenha_nota").hide()
    }
    var d = jq.trim(jq("#" + g + ' textarea[name="comment"]').val());
    if (d.length < 20) {
        jq(".erroResenha_comment_min").show();
        f = true
    } else {
        jq(".erroResenha_comment_min").hide()
    }
    if (d.length > 4000) {
        jq(".erroResenha_comment_max").show();
        f = true
    } else {
        jq(".erroResenha_comment_max").hide()
    }
    if (f) {
        if (g === "form_mini_resenha") {
            jq("#erroMiniResenha").html(jq("#erroResenha").html());
            jq("#erroMiniResenha").slideDown()
        } else {
            jq("#erroResenha").slideDown()
        }
        return false
    } else {
        jq("#submit_resenha").attr("disabled", true);
        jq("#erroResenha, #erroMiniResenha").hide();
        return true
    }
};
