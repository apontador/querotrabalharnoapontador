    r.mousemove(function (a) {
        jq(this).find(".tooltip_selo").css({
            top: a.clientY - 80,
            left: a.clientX - 200
        }).stop(true, true).show()
    });
    r.mouseleave(function () {
        jq(this).find(".tooltip_selo").hide()
    });
    var C = jq(".tooltip_checkin");
    C.mousemove(function (a) {
        jq(this).parent().find(".tooltip_selo").css({
            top: a.clientY - 80,
            left: a.clientX - 200
        }).stop(true, true).show()
    });
    C.mouseleave(function () {
        jq(this).parent().find(".tooltip_selo").hide()
    });
    if (jq("#isPremium").length === 1) {
        jq("#tab_descricao_local a").click()
    } else {
        jq("#tab_avaliacao a").click()
    }
    var t = jq(document).getUrlParam("comment");
    if (t !== null) {
        jq("#tab_avaliacao a").click();
        if (t === "fail") {
            scrollToReviewBox()
        }
    }
    if (jq(document).getUrlParam("photoUploadSuccess") !== null) {
        jq("#tab_fotos a").click();
        scrollToAbas()
    }
    if (jq(document).getUrlParam("photoAba") !== null) {
        jq("#tab_fotos a").click();
        scrollToAbas()
    }
    jq(".enlarge").click(function (b) {
        var a = jq(".mapa_pequeno");
        var c = jq(".mapa_grande");
        var d = c.height();
        if (!jq(".mapa_grande iframe").attr("src")) {
            jq(".mapa_grande iframe").attr("src", MAPLINK_URL + "widget/route_widget1?w=960&h=360&lat=" + jq("#poi_coordinate_y").val() + "&lng=" + jq("#poi_coordinate_x").val() + "&zcontrol=big&mcontrol=yes&dn=" + encodeURIComponent(jq("#poiNameInput").val()))
        }
        c.slideDown();
        jq(".mapa_pequeno img, .mapa_pequeno .footer_veja").hide();
        a.css({
            position: "absolute",
            left: a.offset().left,
            top: a.offset().top
        }).animate({
            left: c.position().left,
            top: c.position().top,
            width: c.width(),
            height: d
        }, function () {
            c.removeClass("transparent");
            a.fadeOut()
        })
    });
    jq(".mapa_grande .reduce").click(function (a) {
        jq(".mapa_grande").slideUp("slow", function () {
            jq(this).addClass("transparent")
        });
        jq(".mapa_pequeno").removeAttr("style").hide().slideDown();
        jq(".mapa_pequeno img, .mapa_pequeno .footer_veja").show()
    });
    var B = false;
    var p = (jq(".parceiro_inline").length === 0) ? true : false;
    if (jq("#isMother").val() === 1) {
        jq.post("/local/getPoiContentPartners.html", {
            lbs_id: jq("#poiId").val()
        }, function (b) {
            if (b.length > 0) {
                if (p === true) {
                    jq("div.conteudo_abas_poi:first").addClass("aba_des_parceiro");
                    jq(".parceiros").fadeIn("slow");
                    if (b.length > 8) {
                        jq(".anterior, .mais").removeClass("hide");
                        B = true
                    }
                }
            }
            jq(b).each(function (f, d) {
                link = '<a href="/local/redirect?partner=' + d.NOME + "&url=" + encodeURIComponent(d.URL) + '" title="' + d.NOME + '">';
                link += '<img src="' + d.FOTO + '" alt="' + d.NOME + '" />';
                link += "</a>";
                jq("div.parceiros .logos .items").append(link)
            });
            var a = jq(".scrollable_icons .items a").clone();
            a = jq.array_chunk(a, 8);
            var c = "";
            jq.each(a, function (f, d) {
                c += "<div>";
                jq(d).each(function (g, h) {
                    c += '<a href="' + jq(h).attr("href") + '" title="' + jq(h).attr("title") + '" target="_blank" onclick="pageTracker._trackPageview(\'/contabilizacao/poi/' + jq(h).attr("title") + "/Mais_informacoes.html');\">" + jq(h).html() + "</a>"
                });
                c += "</div>"
            });
            jq(".scrollable_icons .items").html(c);
            jq("img").error(function () {
                jq(this).attr("src", "/apontador_v8/images/v10/icone_parceiro_quebrado.png")
            });
            if (B && p === true) {
                jq(".scrollable_icons").scrollable({
                    circular: true,
                    next: ".mais",
                    prev: ".anterior"
                })
            }
        })
    } else {
        jq("div.conteudo_abas_poi").removeClass("aba_des_parceiro")
    }
    if (jq(document).getUrlParam("utm_source") === "email") {
        var q = jq('<img src="/apontador_v8/images/v10/seta-avaliacao.png" width="340" />');
        jq("body").append(q);
        jq("#foco_avaliacao").highlight({
            messageElement: q,
            pixelMargin: -15
        })
    }
    jq(".lista_facilidades_poi .tips").click(function (b) {
        b.preventDefault();
        if (!jq(this).hasClass("off")) {
            return
        }
        jq(".parceiro_facilidade_poi").hide();
        var a = jq(this).attr("rel");
        jq("#" + a).fadeIn("fast");
        jq(".lista_facilidades_poi .tips").addClass("off");
        jq(this).removeClass("off")
    })
});
