recommendSuccess = function (o, m) {
    try {
        jq("#rec_message").show()
    } catch (n) {}
    var p = 0;
    jq("#thumbsTotal_" + m).val((parseInt(jq("#thumbsTotal_" + m).val(), 10) + 1));
    var e = parseInt(jq("#thumbsUp_" + m).val(), 10);
    var k = parseInt(jq("#thumbsTotal_" + m).val(), 10);
    if (o === "yes") {
        p = Math.round(((e + 1) / (k)) * 100);
        jq("#thumbsUp_" + m).val((parseInt(jq("#thumbsUp_" + m).val(), 10) + 1));
        jq("button.sprite_bt_rec_up").html('<span class="sprite_icones sprite_ico_rec_up">&nbsp;</span> <span class="left pt2">Obrigado!</span>')
    } else {
        p = Math.round(((e) / (k)) * 100)
    }
    var l = (k === 1) ? " voto" : " votos";
    jq(".percent_votos").fadeOut().html(p + "%").fadeIn();
    recomendationWord = (jq("#thumbsUp_" + m).val() === 1) ? "recomendação" : "recomendações";
    jq("li.rec_total").fadeOut().html('<span class="sprite_icones sprite_recomendacao">&nbsp;</span> <strong>' + jq("#thumbsUp_" + m).val() + "</strong> " + recomendationWord).fadeIn();
    jq("div.cont_votos p").fadeOut().html(l).fadeIn();
    var j = k.toString().split("");
    jq("div.qt_votos span").fadeOut("fast", function () {
        jq("div.qt_votos span").remove();
        var a = 0;
        for (a = 0; a < j.length; a++) {
            jq("div.qt_votos").append("<span>" + j[a] + "</span>")
        }
        jq("div.qt_votos span").last().addClass("last");
        jq("div.qt_votos span").fadeIn()
    })
};
disableRecBtn = function (b) {
    jq(".sprite_bt_rec_up").unbind("click").css("cursor", "default");
    jq(".sprite_bt_rec_down").unbind("click").css("cursor", "default")
};
jq(document).ready(function () {
    jq(".sprite_bt_rec_up").click(function () {
        var d = this;
        var c = jq(d).attr("id").replace("rec_up_", "");
        disableRecBtn(c);
        jq.post("/local/recommend/yes/" + c, "", function (a) {
            if (a.message === "ok") {
                recommendSuccess("yes", c);
                jq(d).addClass("thumbsUpSelected").addClass("voted")
            }
        }, "json")
    });
    jq(".sprite_bt_rec_down").click(function (f) {
        var e = this;
        var d = jq(e).attr("id").replace("rec_down_", "");
        disableRecBtn(d);
        jq.post("/local/recommend/no/" + d, "", function (b, a, c) {
            if (b.message === "ok") {
                recommendSuccess("no", d);
                jq(e).addClass("thumbsDownSelected").addClass("voted")
            }
        }, "json")
    })
});
CrossLoader = {
    status_msg: "",
    call: function (f, e) {
        this.status_msg = e;
        var d = document.createElement("script");
        d.src = "http://www.aponta.me/add?wt=xss&url=" + f;
        jq("body").append(d)
    },
    callback: function (c) {
        var d = this.status_msg.replace("%shorturl%", c.shortURL);
        window.location.href = "http://twitter.com/home?status=" + d
    }
};
var apontame = Class.create({
    initialize: function (h, l, g, i, j, k) {
        this.name = h;
        this.adress = l;
        this.state = g;
        this.urlImage = i;
        this.shortUrl = false;
        this.url = k;
        this.id = j;
        this.registerListeners()
    },
    registerListeners: function () {},
    openShareBox: function (b) {
        jq(".share_box").slideToggle();
        if (!this.shortUrl) {
            this.generateApontame(this)
        }
        $("poiShortUrl").setValue(this.shortUrl)
    },
    setApontameUrl: function (b) {
        this.shortUrl = b
    },
    generateApontame: function (b) {
        Ajax.Request("/local/apontame.html", {
            parameters: {
                url: b.url
            },
            asynchronous: false,
            onComplete: function (d) {
                var a = d.responseJSON;
                b.shortUrl = a.url
            },
            onFailure: function () {}
        })
    },
    setMessage: function (b) {
        b.message = "Eu recomendo " + b.name + " em " + b.state + ". E você? " + b.shortUrl + " no @apontador #apontame"
    },
    setData: function (b) {
        if (!b.shortUrl) {
            b.generateApontame(b)
        }
        if (!b.message) {
            b.setMessage(b)
        }
    },
    apontameTwitter: function (b) {
        this.setData(this);
        window.open("http://twitter.com/home?status=" + encodeURIComponent(this.message));
        pageTracker._trackPageview("/contabilizacao/poi/botao_twitter/" + this.id + "/" + this.name);
        Event.stop(b);
        return false
    }
});
var shortUrl = new apontame(poiName, poiAdress, poiState, urlImage, poiId, poiUrl);
var shorted = false;
jq(document).ready(function () {
    jq("#doShare, #closeShare").click(function (c) {
        jq(".share_box").slideToggle();
        jq.scrollTo("h1", 1000);
        if (!shorted) {
            shorted = true;
            var d = jq("#poiUrlInput").val();
            jq("#poiShortUrl").val(d);
            jq("#shortening").fadeIn();
            jq.ajax({
                type: "POST",
                url: "/local/apontame.html",
                data: {
                    url: poiUrl
                },
                dataType: "json",
                success: function (b) {
                    if (b.url !== null) {
                        jq("#poiShortUrl").val(b.url);
                        shortUrl.setApontameUrl(b.url);
                        var a = b.url + ".qrcode";
                        jq("#shortening").fadeOut("fast", function () {
                            jq("#qrcode").attr("src", a).fadeIn()
                        })
                    }
                }
            })
        }
        c.preventDefault();
        return false
    })
});