            n.data("img", p)
        }
        var c = w.start.top || Math.round(j.height() / 2),
            d = w.start.left || Math.round(j.width() / 2);
        if (b) {
            b = f(b);
            c = b.top;
            d = b.left
        }
        if (w.fixed) {
            c -= j.scrollTop();
            d -= j.scrollLeft()
        } else {
            x.top += j.scrollTop();
            x.left += j.scrollLeft()
        }
        p.css({
            position: "absolute",
            top: c,
            left: d,
            width: 0,
            zIndex: w.zIndex
        }).show();
        x.position = g;
        n.css(x);
        p.animate({
            top: n.css("top"),
            left: n.css("left"),
