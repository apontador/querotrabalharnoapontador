        }
    };
    b.getFollowing = function (d, a) {
        followingContainer = a;
        if (d.length > 0 && b.isArray(d)) {
            userList = b.ajax({
                url: "/accounts/checkwhoisfollowinginthelist.html",
                type: "POST",
