{
  "parser": "babel-eslint",
  "rules": {
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "eqeqeq": 2,
    "no-console": 0,
    "react/prop-types": 0,
    "no-debugger": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}